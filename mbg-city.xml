<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

	<context id="DB2Tables" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<!-- 配置数据库连接 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://120.26.65.52:3306/city?serverTimezone=GMT%2B8" userId="root"
			password="lyc1289choose">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- 指定javaBean生成的位置 -->
		<javaModelGenerator targetPackage="com.nit.city.bean"
			targetProject=".\src\main\java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<!--指定sql映射文件生成的位置 -->
		<sqlMapGenerator targetPackage="mapper" targetProject=".\src\main\resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<!-- 指定dao接口生成的位置，mapper接口 -->
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.nit.city.dao" targetProject=".\src\main\java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>


		<!-- table指定每个表的生成策略 -->
<!--		<table tableName="city_user" domainObjectName="User"></table>-->
<!--		<table tableName="city_manager" domainObjectName="Manager"></table>-->
<!--		<table tableName="city_camera" domainObjectName="Camera"></table>-->
<!--		<table tableName="city_coordinate" domainObjectName="Coordinate"></table>-->
<!--		<table tableName="city_image" domainObjectName="Image"></table>-->
		<table tableName="city_message" domainObjectName="Message"></table>
<!--		<table tableName="city_position" domainObjectName="Position"></table>-->
<!--		<table tableName="city_city" domainObjectName="City"></table>-->
<!--		<table tableName="city_province" domainObjectName="Province"></table>-->
<!--		<table tableName="city_district" domainObjectName="District"></table>-->
<!--		<table tableName="city_result" domainObjectName="Result"></table>-->
	</context>
</generatorConfiguration>